use teste;
create table pessoas (
id int not null auto_increment primary key,
nome varchar(30),
nasc date,
sexo enum('M', 'F'),
peso dec(5,2),
altura dec(3,2),
nacionalidade varchar(20) default 'Brasil'
)default charset = utf8mb4;
desc pessoas;
describe pessoas;
INSERT INTO pessoas 
(nome, nasc, sexo, altura, nacionalidade)
 VALUES 
